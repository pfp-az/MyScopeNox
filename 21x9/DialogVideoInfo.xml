<?xml version="1.0" encoding="UTF-8"?>
<!-------------------------------------------------------------------------------------------------------------------------------->
<!-- This is the file responsible for displaying the video info details.                                                        -->
<!-------------------------------------------------------------------------------------------------------------------------------->
<window>
	<defaultcontrol always="true">8</defaultcontrol>
	<allowoverlay>no</allowoverlay>
